{
  "name": "@uvue/rquery",
  "version": "1.0.0",
  "description": "Helper to traverse and manipulate AST with recast",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && tsc",
    "watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint --fix 'src/**/*.ts'",
    "format": "prettier --ignore-path .gitignore --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "recast",
    "query",
    "ast"
  ],
  "author": "Simon Babay <simon@babay.fr>",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.4.5",
    "@types/node": "^16.16.0",
    "lodash": "^4.17.11",
    "prettier": "^1.17.1",
    "recast": "^0.18.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "tslint": "^5.16.0"
  }
}
